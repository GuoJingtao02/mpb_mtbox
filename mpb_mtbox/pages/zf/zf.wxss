.fl{
  float: left;
}
.fr{
  float: right;
}
page{
   background: #f0f0f0;
   
}

.title{
  text-align: center;
  background: #f0f0f0;
  padding: 34rpx 0;
  font-size: 30rpx;
 
}
.title_name{
  height: 48rpx;
  line-height: 48rpx;
  width: 220rpx;
  border-radius: 24rpx;
  background: #6cdbd6;
  margin: 0 auto;
   position: relative;
}
.title navigator{
  position: absolute;
  top: 40rpx;
  right: 10%;
  background: #f0f0f0;
  color: #968303;
  text-decoration: underline;
  margin-left: 20rpx;
  font-size: 24rpx;
}
.title .desc{
  color: #3b5250;
  font-size: 24rpx;
  height: 48rpx;
  line-height: 48rpx;
}
.price{
  background: #fff;
  margin-top: 29rpx;
  font-size: 26rpx;
  overflow: hidden;
  padding: 20rpx 0;
}
.price .li{
  width: 557rpx;
  margin: 0 auto;
  clear: both;
  height: 58rpx;
  line-height: 58rpx;
}
.protocol{
  margin-top: 29rpx;
  margin-left: 96rpx;
   font-size: 24rpx;
   width: 557rpx;
   color: #3b3a3a;
}
.protocol navigator{
  display: inline;
  text-decoration: underline;
}
.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  height: 106rpx;
  width: 100%;
}
.footer .fl{
  height: 106rpx;
  width: 449rpx;
  padding-left: 43rpx;
}
.footer .fl .tp{
  font-size: 36rpx;
  line-height: 68rpx;
  height: 68rpx;
}
.footer .fl .tp text{
  color: #ff544b;
} 
.footer .fl .yj{
  font-size: 20rpx;
 
}
.footer .fr{
  background: #ff544b;
  width: 256rpx;
  height: 106rpx;
  line-height: 106rpx;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
}


.li{
  width: 100%;
  height: 252rpx;
}
.li1{
  width: 446rpx;
  margin: 0 auto;
  border-radius: 10rpx;
  height: 252rpx;
  transform: scale(0.9);
}
.selected{
  transform: scale(1)
}
.plan_name{
  padding: 45rpx 0 0 64rpx;
  font-size: 28rpx;
  line-height: 36rpx;
  height: 36rpx;
}
.service_time{
  padding-left: 64rpx;
  font-size: 24rpx;
  line-height: 32rpx;
  height: 32rpx;
}