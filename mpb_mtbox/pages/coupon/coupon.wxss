page{background: #f2f2f2;}

.exchangeWrap{padding: 10.4px;overflow: hidden;}
.ehTxt-parent{width: 75%;float: left;}
.ehTxt{width: 100%;height:26px;padding-left:3px;border: 1px solid #ccc;border-radius:4px;box-sizing: border-box;-webkit-box-sizing: border-box;background:#fff;}
.btn-parent{width: 25%;float: left;}
.ehBtn{width: 100%;height:26px;border: 1px solid #ddd;color: #299779;background: #ddd;line-height: 26px;text-align: center;}
.couponTab,.orderTab{background: #fff;}
.couponTab li,.orderTab li{float: left;width: 33%;height: 41.6px;line-height: 41.6px;text-align: center;}
.couponTab li.current,.orderTab li.current{border-bottom: 2px solid #5DAE98;}



.expired{padding-top: 8px;}
.expired .cpItem{position:relative;width:100%;box-sizing:border-box;padding:0 10.4px 0 10.4px;}
.cp-item-child{background:url(https://www.72toy.com/mpb_mtbox/v/v3/share/images/memCenter/coupon_bg2.png) no-repeat left 0;background-size:100% 56.16px;padding:10.4px;}

.nouse .cp-item-child{background:url(https://www.72toy.com/mpb_mtbox/v/v3/share/images/memCenter/coupon_bg1.png) no-repeat left 0;background-size:100% 56.16px;padding:10.4px;}

.cpItem .useTerm{color: #8D8D8D;}
.cpItem .fn-left{width: 66.66%}
.cpItem .fn-right{width: 33.33%;position: relative;}
.cpItem .used_icon{position: absolute;top: -5.2px;right: -5.2px;}
.cpItem .used_icon image{width: 39.52px;height: 39.52px;}
.cpMoney{width:121.16px;color: white; text-align: right;margin: -6.76px 0 0 0;}
.cpMoney p{padding-right: 10.4px;}
.cpMoney .money{font-size: 24.96px;}